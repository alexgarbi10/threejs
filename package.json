{
  "name": "threejs-project",
  "version": "1.0.0",
  "description": "A modern Three.js 3D visualization project featuring a rotating torus knot with texture mapping",
  "type": "module",
  "main": "./app/js/main.js",
  "module": "./app/js/main.js",
  "files": [
    "app",
    "textures",
    "index.html",
    "vite.config.js",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,css,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,css,html,md}\"",
    "type-check": "echo 'TypeScript not configured yet'",
    "clean": "rm -rf dist node_modules/.vite",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "three": "^0.169.0"
  },
  "devDependencies": {
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.15.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {},
  "optionalDependencies": {},
  "bundledDependencies": [],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "keywords": [
    "threejs",
    "three.js",
    "3d",
    "webgl",
    "visualization",
    "graphics",
    "rendering",
    "torus-knot",
    "geometry",
    "texture-mapping",
    "animation",
    "interactive",
    "canvas",
    "modern",
    "es6",
    "vite"
  ],
  "author": {
    "name": "Alejandro Garbi",
    "url": "https://github.com/alexgarbi10"
  },
  "contributors": [],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexgarbi10/threejs.git",
    "directory": ""
  },
  "bugs": {
    "url": "https://github.com/alexgarbi10/threejs/issues"
  },
  "homepage": "https://github.com/alexgarbi10/threejs#readme",
  "funding": {},
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "port": 9000,
    "open": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "preferGlobal": false,
  "directories": {
    "lib": "app/js",
    "test": "app/js"
  }
}
