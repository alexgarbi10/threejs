{
  "name": "threejs-project",
  "version": "1.0.0",
  "description": "A modern Three.js 3D visualization project",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,json,css,html}\""
  },
  "dependencies": {
    "three": "^0.169.0"
  },
  "devDependencies": {
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.15.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "threejs",
    "3d",
    "webgl",
    "visualization"
  ],
  "author": "Alejandro Garbi",
  "license": "MIT"
}
